{
	"version": 2,
	"buildCommand": "bun run build",
	"devCommand": "bun run start:dev",
	"installCommand": "bun install",
	"outputDirectory": "dist",
	"builds": [
		{
			"src": "dist/main.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/main.js",
			"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
		}
	]
}
